<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cadastrar Cachorro"
        android:id="@+id/btn_cachorro"
        android:onClick="cadastrarCachorro"
        />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Listar Cachorros"
        app:layout_constraintTop_toBottomOf="@id/btn_cachorro"
        android:onClick="listarCachorros"
        />



</androidx.constraintlayout.widget.ConstraintLayout>